      </Label>
      <Label fx:id="usernameLabel" layoutX="18.0" layoutY="67.0" text="Username">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Pane layoutX="13.0" layoutY="100.0" prefHeight="282.0" prefWidth="576.0">
         <children>
            <Label fx:id="typeLabel" layoutX="14.0" layoutY="14.0" prefHeight="20.0" prefWidth="256.0" text="Request type:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="idLabel" layoutX="14.0" layoutY="44.0" prefHeight="20.0" prefWidth="256.0" text="From user:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="nameLabel" layoutX="14.0" layoutY="74.0" prefHeight="20.0" prefWidth="256.0" text="Full name:">
